<h2>Images</h2>

<% @images.each do |image| %>
  <img src="<%= image.url %>">
  <%= image.alt_tag %>
  <%= link_to "Edit", edit_admin_image_path(image) %> - <%= link_to "Delete", admin_image_path(image), method: :delete %>
<% end %>

<footer>
  <h3>Add Image</h3>
  <%= render partial: "form" %>
</footer>
